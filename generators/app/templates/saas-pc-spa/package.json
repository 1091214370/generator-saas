{
  "name": "web-boh-org",
  "version": "1.0.0",
  "description": "pc人员组织结构前端应用",
  "author": "",
  "repository": {
    "type": "git",
    "url": ""
  },
  "scripts": {
    "clean": "rimraf www _dist _package",
    "precommit": "lint-staged",
    "commitmsg": "commitlint -e",
    "lint": "npm run lint:js && npm run lint:style",
    "lint:js": "eslint ./src --ext '.ts,.tsx,.js,.jsx'",
    "lint:style": "stylelint \"**/*.less\" --syntax less"
  },
  "dependencies": {
    "@ali/saas-plugin-fireworm": "1",
    "@alipay/kos-wrapper-pc": "1",
    "@alipay/mo-alsh-merchant-framework": "1",
    "antd": "3",
    "classnames": "2",
    "single-spa": "4",
    "single-spa-react": "2",
    "systemjs": "4",
    "typescript": "^3.5.3"
  },
  "devDependencies": {
    "@alipay/commitlint-config-kobe": "2",
    "@alipay/eslint-config-kobe": "2",
    "@alipay/stylelint-config-kobe": "2",
    "@commitlint/cli": "6",
    "@typescript-eslint/eslint-plugin": "^1.11.0",
    "@typescript-eslint/parser": "^1.11.0",
    "eslint": "4",
    "husky": "0",
    "lint-staged": "7",
    "rimraf": "2",
    "stylelint": "9"
  },
  "engines": {
    "install-node": "8"
  },
  "publishConfig": {
    "registry": "http://registry.npm.alibaba-inc.com"
  },
  "commitlint": {
    "extends": [
      "@alipay/commitlint-config-kobe"
    ]
  },
  "lint-staged": {
    "*.js": "eslint",
    "*.less": "stylelint --syntax less"
  }
}
